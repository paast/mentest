extends base.pug

block content
  h2 ADD RUN
  form(class='test-form' method='post')
    label(for='name') name
    input(type='text' name='name')
    label(for='runId') runId
    input(type='text' name='runId')
    label(for='isParallel') isParallel
    input(type='checkbox' name='isParallel')
    label(for='numThreads') numThreads
    input(type='text' name='numThreads')
    input(type='submit')
